{
  "name": "@rigwild/apidoc-markdown",
  "version": "1.2.0",
  "description": "Generate API documentation in Markdown format from apiDoc data.",
  "main": "index.js",
  "bin": {
    "apidoc-markdown": "./index.js"
  },
  "directories": {
    "example": "examples"
  },
  "keywords": [
    "apidoc",
    "markdown"
  ],
  "scripts": {
    "help": "node index.js -h",
    "example": "node index.js -p examples -o examples/example.md",
    "example-multi": "node index.js -p examples -o examples/multi-arg --multi --createPath"
  },
  "dependencies": {
    "ejs": "^2.6.1",
    "yargs": "^13.2.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/rigwild/apidoc-markdown"
  },
  "bugs": {
    "url": "https://github.com/rigwild/apidoc-markdown/issues"
  },
  "author": "rigwild <contact@asauvage.fr> (https://github.com/rigwild)",
  "license": "MIT"
}
