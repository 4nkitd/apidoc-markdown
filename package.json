{
  "name": "@rigwild/apidoc-markdown",
  "version": "2.0.2",
  "description": "Generate API documentation in Markdown format from apiDoc data.",
  "main": "lib/index.js",
  "bin": {
    "apidoc-markdown": "./bin/index.js"
  },
  "directories": {
    "example": "examples",
    "lib": "lib",
    "test": "test"
  },
  "keywords": [
    "apidoc",
    "markdown",
    "programmatic"
  ],
  "scripts": {
    "help": "node bin/index.js -h",
    "example": "node bin/index.js -p examples -o examples/example.md",
    "example-multi": "node bin/index.js -p examples -o examples/multi-arg --multi --createPath",
    "prog:example": "node examples/generateSingle.js -p examples -o examples/example.md",
    "prog:example-multi": "node examples/generateMulti.js -p examples -o examples/multi-arg --multi --createPath",
    "test": "mocha"
  },
  "dependencies": {
    "ejs": "^2.6.1",
    "semver": "^6.1.1",
    "yargs": "^13.2.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/rigwild/apidoc-markdown"
  },
  "bugs": {
    "url": "https://github.com/rigwild/apidoc-markdown/issues"
  },
  "author": "rigwild <contact@asauvage.fr> (https://github.com/rigwild)",
  "license": "MIT",
  "devDependencies": {
    "eslint": "^5.16.0"
  }
}
