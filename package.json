{
  "name": "apidoc-markdown",
  "version": "5.0.2",
  "description": "üìù Generate API documentation in Markdown from apiDoc data",
  "license": "MIT",
  "repository": "rigwild/apidoc-markdown",
  "author": {
    "name": "rigwild",
    "email": "me@rigwild.dev",
    "url": "https://rigwild.dev"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "apidoc-markdown": "./bin/index.js"
  },
  "files": [
    "bin",
    "dist",
    "templates"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.prod.json",
    "help": "node bin/index.js -h",
    "example:apidoc": "cd example/_apiDocData && rm -rf out && apidoc -i src -o out",
    "example:generate": "ts-node example/generateExamples.ts",
    "example:clean": "rm -rf example/basic/example.md example/multi/output/",
    "test": "ava --verbose",
    "prepublishOnly": "tsc -p tsconfig.prod.json"
  },
  "dependencies": {
    "ejs": "^3.0.2",
    "semver": "^7.3.2",
    "update-notifier": "^4.1.0",
    "yargs": "^15.3.1"
  },
  "devDependencies": {
    "@types/ejs": "^3.0.2",
    "@types/node": "^13.11.1",
    "@types/semver": "^7.1.0",
    "@types/yargs": "^15.0.4",
    "apidoc": "^0.20.1",
    "ava": "^3.7.0",
    "ts-node": "^8.8.2",
    "typescript": "^3.8.3"
  },
  "keywords": [
    "apidoc",
    "markdown",
    "portable",
    "api",
    "doc",
    "documentation",
    "generator",
    "programmatic",
    "cli",
    "cli-app",
    "command-line",
    "rest",
    "restful"
  ],
  "ava": {
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register"
    ]
  }
}
