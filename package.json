{
  "name": "@rigwild/apidoc-markdown",
  "version": "4.0.0",
  "description": "Generate API documentation in Markdown format from apiDoc data",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "apidoc-markdown": "./bin/index.js"
  },
  "directories": {
    "example": "examples",
    "lib": "src",
    "test": "test"
  },
  "keywords": [
    "apidoc",
    "markdown",
    "api-documentation",
    "generator",
    "programmatic",
    "command-line"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.prod.json",
    "help": "node bin/index.js -h",
    "example:apidoc": "cd examples/_apiDocData && rm -rf out && apidoc -i src -o out",
    "example:generate": "ts-node examples/generateExamples.ts",
    "example:clean": "rm -rf examples/basic/example.md examples/multi/output/ examples/ordered/example.md",
    "test": "ava --verbose"
  },
  "dependencies": {
    "ejs": "^3.0.1",
    "semver": "^7.1.3",
    "yargs": "^15.1.0"
  },
  "devDependencies": {
    "@types/ejs": "^3.0.1",
    "@types/node": "^13.7.6",
    "@types/semver": "^7.1.0",
    "@types/yargs": "^15.0.4",
    "apidoc": "^0.20.0",
    "ava": "^3.4.0",
    "ts-node": "^8.6.2",
    "typescript": "^3.8.2"
  },
  "ava": {
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/rigwild/apidoc-markdown"
  },
  "bugs": {
    "url": "https://github.com/rigwild/apidoc-markdown/issues"
  },
  "author": "rigwild <me@rigwild.dev> (https://rigwild.dev/)",
  "contributors": [
    "martinj (https://github.com/martinj)",
    "softdevstory (https://github.com/softdevstory)",
    "vanekj (https://github.com/vanekj)"
  ],
  "license": "MIT"
}
